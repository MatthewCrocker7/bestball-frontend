{
  "name": "bestball-frontend",
  "version": "1.0.0",
  "description": "Front end for best ball app",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node conf/server.js",
    "start-dev": "npm run build-dev && node conf/server.js",
    "start-uat": "npm run build-uat && node conf/server.js",
    "build": "webpack --mode production --env=PROD",
    "build-dev": "webpack --mode production --env=DEV",
    "build-uat": "webpack --mode production --env=UAT",
    "dev": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "build-package-css": "cp src/assets/css/material-dashboard-react.css dist/material-dashboard-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/material-dashboard-react.git"
  },
  "keywords": [],
  "author": "Matthew Crocker <matthewcroc@gmail.com>)",
  "license": "MIT",
  "optionalDependencies": {
    "@types/googlemaps": "3.39.6",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "6.12.2",
    "typescript": "3.9.3"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "4.10.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "date-fns": "^2.15.0",
    "express": "^4.17.1",
    "history": "4.10.1",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-chartist": "0.14.3",
    "react-dom": "16.13.1",
    "react-google-maps": "9.4.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-swipeable-views": "0.13.9",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-import-rename": "^1.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "^2.0.2",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "2.0.5",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  }
}
